import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        Scanner ler = new Scanner(System.in);

        System.out.print("Digite o numero maior que 1000: ");
        int menorNumero = ler.nextInt();

        System.out.print("Digite o numero menor que 1000: ");
        int maiorNumero = ler.nextInt();

        System.out.println("NÃºmeros no intervalo [" + menorNumero + ", " + maiorNumero + "] " +
                "que divididos por 11 resultam em resto 5:");

        for (int i = menorNumero; i <= maiorNumero; i++) {
            if (i % 11 == 5) {
                System.out.println(i);
            }
        }


    }
    }
